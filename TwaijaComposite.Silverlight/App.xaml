        									<RadialGradientBrush>
        										<GradientStop Color="White" Offset="0.961"/>
        										<GradientStop Color="Black" Offset="0.668"/>
        										<GradientStop Color="Black" Offset="0.996"/>
        									</RadialGradientBrush>
        								</Ellipse.Stroke>
        							</Ellipse>
        							<Ellipse x:Name="CheckIcon" Height="8" Opacity="0" Width="8">
